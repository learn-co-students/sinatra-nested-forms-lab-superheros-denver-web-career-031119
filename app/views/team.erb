<%=params["team"]["name"]%><br>
Team Motto: <%=params["team"]["motto"]%><br>

<%params["team"]["heroes"].each do |hero|%>
  <%hero.each do |a,v|%>
    <%case a%>
    <%when a.include?("name")%>Hero Name:
    <%when a.include?("power")%>Hero Power:
    <%when a.include?("bio")%>Hero Bio:
    <%end%>
    <%=v%><br>
  <%end%>
<%end%>
